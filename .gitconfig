[include]
	path = ~/.gitconfig-core
	path = ~/.gitconfig-diff
	path = ~/.gitconfig-aliases

[user]
	name = Jace Browning
	email = jacebrowning@gmail.com

[init]
	defaultBranch = main

[hub]
	protocol = https

[credential]
	helper = osxkeychain

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process

[core]
	excludesfile = ~/.gitignore_global
	editor = vim
